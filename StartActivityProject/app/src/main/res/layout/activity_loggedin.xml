<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoggedinActivity"
    android:orientation="vertical"
    android:weightSum="100"
    android:background="@color/backgroundLogg">

<LinearLayout
    android:layout_weight="35"
    android:layout_margin="10dp">
    <ImageView
        android:id="@+id/drinks"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/drinkss"
        android:layout_weight="25"
        android:layout_marginRight="5dp"/>
    <ImageView
        android:id="@+id/food"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/foodd"
        android:layout_weight="25"
        android:layout_marginLeft="5dp"/>
</LinearLayout>

<LinearLayout
    android:layout_weight="35"
    android:layout_marginRight="10dp"
    android:layout_marginLeft="10dp">

    <ImageView
        android:id="@+id/diet"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/customce"
        android:layout_weight="25"
        android:layout_marginRight="5dp"/>
    <ImageView
        android:id="@+id/bmi"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/vagaa"
        android:layout_weight="25"
        android:layout_marginLeft="5dp"/>
</LinearLayout>

<LinearLayout
    android:layout_weight="30"
    android:layout_margin="10dp">
    <ImageView
        android:id="@+id/maps"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/map"
        android:layout_weight="25"/>
</LinearLayout>


</TableLayout>